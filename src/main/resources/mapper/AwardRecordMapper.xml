<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.chy.crud.mapper.AwardRecordMapper">


    <select id="selectAwardRecord" resultType="org.chy.crud.pojo.AwardRecord"
            parameterType="java.lang.Integer">
        select * from award_record where user_id = #{userId}
    </select>
